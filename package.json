{
  "name": "@hello.nrfcloud.com/map-backend",
  "version": "0.0.0-development",
  "description": "Cloud backend for hello.nrfcloud.com/map developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "npx globstar -- npx tsx --test --test-reporter spec \"!(dist|node_modules)/**/*.spec.ts\"",
    "test:e2e": "npx tsx --no-warnings ./feature-runner/run-features.ts > e2e-test-result.json && cat e2e-test-result.json | npx tsx --no-warnings ./feature-runner/console-reporter.ts --only-failed --with-timestamps",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/map-backend.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/map-backend/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/map-backend#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-acm": "3.590.0",
    "@aws-sdk/client-cloudwatch-logs": "3.590.0",
    "@aws-sdk/client-dynamodb": "3.590.0",
    "@aws-sdk/client-ecr": "3.590.0",
    "@aws-sdk/client-iot-data-plane": "3.590.0",
    "@bifravst/aws-cdk-ecr-helpers": "1.2.17",
    "@bifravst/aws-cdk-lambda-helpers": "1.7.41",
    "@bifravst/aws-ssm-settings-helpers": "1.1.26",
    "@bifravst/ci": "2.0.1",
    "@bifravst/eslint-config-typescript": "6.1.2",
    "@bifravst/http-api-mock": "1.4.73",
    "@bifravst/prettier-config": "1.0.2",
    "@bifravst/run": "1.2.0",
    "@commitlint/config-conventional": "19.2.2",
    "@hello.nrfcloud.com/bdd-markdown-steps": "2.4.76",
    "@nordicsemiconductor/bdd-markdown": "8.1.15",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.3",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.25",
    "@types/aws-lambda": "8.10.138",
    "@types/lodash-es": "4.17.12",
    "aws-cdk": "2.144.0",
    "aws-cdk-lib": "2.144.0",
    "cdk": "2.144.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "commander": "12.1.0",
    "commitlint": "19.3.0",
    "constructs": "10.3.0",
    "globstar": "1.0.0",
    "husky": "9.0.11",
    "jsonata": "2.0.5",
    "semver": "7.6.2",
    "tsmatchers": "5.0.2",
    "tsx": "4.11.2"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "dependencies": {
    "@aws-lambda-powertools/metrics": "2.1.1",
    "@aws-sdk/client-lambda": "3.590.0",
    "@aws-sdk/client-ses": "3.590.0",
    "@aws-sdk/client-ssm": "3.590.0",
    "@aws-sdk/util-dynamodb": "3.590.0",
    "@hello.nrfcloud.com/certificate-helpers": "1.0.0",
    "@hello.nrfcloud.com/lambda-helpers": "1.1.26",
    "@hello.nrfcloud.com/nrfcloud-api-helpers": "3.0.58",
    "@hello.nrfcloud.com/proto": "12.0.4",
    "@hello.nrfcloud.com/proto-map": "9.2.0",
    "@middy/core": "5.4.0",
    "@nordicsemiconductor/from-env": "3.0.1",
    "@nordicsemiconductor/random-words": "8.0.1",
    "@nordicsemiconductor/timestream-helpers": "6.0.2",
    "@sinclair/typebox": "0.32.31",
    "id128": "1.6.6",
    "lodash-es": "4.17.21"
  }
}
